<Patch>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>RCP.Genes</modId>
		<key>RCP_RemoveImpid</key>
		<defaultValue>false</defaultValue>
		<caseTrue>
			<Operation Class="PatchOperationRemove">
				<xpath>//FactionDef[defName="TribeSavageImpid"] | //PawnKindDef[defaultFactionDef/text()="TribeSavageImpid"] | //XenotypeDef/doubleXenotypeChances/Impid | //xenotypeSet/xenotypeChances/Impid</xpath>
			</Operation>
			<Operation Class="PatchOperationAdd">
				<xpath>//XenotypeDef[defName="Impid"]</xpath>
				<value>
					<factionlessGenerationWeight>0</factionlessGenerationWeight>
				</value>
			</Operation>
		</caseTrue>
	</Operation>
</Patch>