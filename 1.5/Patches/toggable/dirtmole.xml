<Patch>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>True</enabled>
		<label>remove dirtmoles</label>
		<operations>
			<li Class="PatchOperationRemove">
				<xpath>Defs/XenotypeDef/doubleXenotypeChances/Dirtmole</xpath>
			</li>
			<li Class="PatchOperationRemove">
				<xpath>Defs/FactionDef/xenotypeSet/xenotypeChances/Dirtmole</xpath>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/XenotypeDef[defName="Dirtmole"]</xpath>
				<value>
					<factionlessGenerationWeight>0</factionlessGenerationWeight>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>