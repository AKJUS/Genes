<Patch>
    <Operation Class="ModSettingsFramework.PatchOperationModOption">
        <defaultValue>false</defaultValue>
        <label>Add highmates to the empire</label>
        <id>RCP_GenesEmpireHighmates</id>
        <category>RCP_GenesPatches</category>
        <order>20</order>
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>//FactionDef[defName="Empire"]/xenotypeSet/xenotypeChances</xpath>
				<value>
					<Highmate>0.025</Highmate>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>